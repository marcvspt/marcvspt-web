{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/sections/*": [
        "src/sections/*"
      ],
      "@/icons/*": [
        "src/icons/*"
      ],
      "@/scripts/*": [
        "src/scripts/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ]
    }
  }
}